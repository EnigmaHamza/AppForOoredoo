@model IEnumerable<ooredooApplicationForWeb.Data.Observations>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/admin/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.user)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.categorie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.typeSuivie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rating)
            </th>

            <th>
                Titre
            </th>
            <th>
                @Html.DisplayNameFor(model => model.text)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.user.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.categorie.NomCategorie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.typeSuivie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rating)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.text)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
